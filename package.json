{
  "name": "react-scaffold",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html",
    "test": "jest",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
